@using EntityLayer
@model List<Address>

<div class="w3l_header_left">
    <ul>      
        @for (int i = 0; i < Model.Count; i++)
        {
             <li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>@Model[i].Description1 @Model[i].Description2</li>
            <li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span> @Model[i].Description4</li>
            <li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> <a href="mailto:info@example.com">@Model[i].Description3</a></li>
        }
                   
    </ul>
</div>
