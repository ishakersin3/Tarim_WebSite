@{
    ViewData["Title"] = "Index";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_HeadView");
<body>
    <!-- header -->
    <div class="header">
        <div class="container">
            @await Component.InvokeAsync("_TopAddressView");
            @await Component.InvokeAsync("_SocialMediaView");
            <div class="clearfix"> </div>
        </div>
    </div>

    <!-- //header -->
    <!--Slider-->
    @await Component.InvokeAsync("_NavbarView");
    <div class="modal fade" id="myModal1" tabindex="-1" role="dialog">
        <!-- Modal1 -->
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4>In Harvest</h4>
                    <img src="~/web/images/banner1.jpg" alt=" " class="img-responsive">
                    <h5>ENVIRONMENT & SUSTAINABILITY</h5>
                    <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries, but also the leap into electronic typesetting.</p>
                </div>
            </div>
        </div>
    </div>
    @await Component.InvokeAsync("_AboutView");
    @await Component.InvokeAsync("_ServiceView");

    @await Component.InvokeAsync("_TeamView");

    @await Component.InvokeAsync("_AnnouncementView")
    @await Component.InvokeAsync("_GalleryView")


    <!--//gallery-->
    <!--footer-->
    <div class="contact-agile" id="contact">
        <div class="container">
            @await Component.InvokeAsync("_AddressView")

            @await Html.PartialAsync("/Views/Default/SendMessage.cshtml");
        </div>
    </div>

    <div class="bottom-social-w3ls w3ls-social-icons">
        <a class="facebook" href="#"><i class="fa fa-facebook"></i></a>
        <a class="twitter" href="#"><i class="fa fa-twitter"></i></a>
        <a class="pinterest" href="#"><i class="fa fa-pinterest-p"></i></a>
        <a class="linkedin" href="#"><i class="fa fa-linkedin"></i></a>
    </div>
    <div class="copy">
        <p>© @DateTime.Now.Year Hasat Time . Tüm Hakları Saklıdır | Design by <a>Rs1N</a> </p>
    </div>
    <!--/footer -->
    <!-- js -->
    @await Html.PartialAsync("/Views/Default/Script.cshtml");

</body>
</html>

